{
    "name": "glujs",
    "version": "0.2.2",
    "description": "Simple Utility Library",
    "license": "MIT",
    "author": {
        "name": "Garrit Franke",
        "email": "garritfranke@gmail.com"
    },
    "main": "./public/dist/main.bundle.js",
    "bugs": {
        "url": "https://github.com/garritfra/glujs/issues"
    },
    "homepage": "https://github.com/garritfra/glujs#readme",
    "scripts": {
        "audit": "source-map-explorer public/dist/main.bundle.js",
        "clear": "rm -rf public/dist/",
        "clear:all": "rm -rf node_modules/ && npm run clear",
        "count": "find src -name '*.js' | xargs wc -l | sort -r",
        "build": "webpack -p",
        "watch": "webpack -w"
    },
    "devDependencies": {
        "babel-core": "^6.24.0",
        "babel-loader": "^6.4.1",
        "babel-preset-es2015": "^6.24.0",
        "babel-preset-es2016": "^6.22.0",
        "source-map-explorer": "^1.3.3",
        "webpack": "^2.3.2"
    },
    "keywords": [
        "Node",
        "Math",
        "Utils"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/garritfra/glujs.git"
    }
}
